    void penjumlahan(){
        nilaiA = Integer.parseInt(jTextField1.getText());
        nilaiB = Integer.parseInt(jTextField2.getText());
        nilaiHasil = nilaiA + nilaiB;
        jLabel3.setText(""+nilaiHasil);
    }
    
    void pengurangan(){
        nilai1A = Double.parseDouble(jTextField1.getText());
        nilai1B = Double.parseDouble(jTextField2.getText());
        nilaiHasil1 = nilai1A - nilai1B;
        jLabel3.setText(""+nilaiHasil1);   
    }
    
    void perkalian(){
        nilai2A = Integer.parseInt(jTextField1.getText());
        nilai2B = Integer.parseInt(jTextField2.getText());
        nilaiHasil2 = nilai2A * nilai2B;
        jLabel3.setText(""+nilaiHasil2);
    }
    
    void pembagian(){
        nilai3A = Double.parseDouble(jTextField1.getText());
        nilai3B = Double.parseDouble(jTextField2.getText());
        nilaiHasil3 = nilai3A / nilai3B;
        jLabel3.setText(""+nilaiHasil3);
    }
    
